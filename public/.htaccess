<IfModule mod_rewrite.c>
    <IfModule mod_negotiation.c>
        Options -MultiViews
    </IfModule>

    RewriteEngine On

    # Redirect Trailing Slashes If Not A Folder...
    RewriteCond %{REQUEST_FILENAME} !-d
    RewriteRule ^(.*)/$ /$1 [L,R=301]

    # Handle Front Controller...
    RewriteCond %{REQUEST_FILENAME} !-d
    RewriteCond %{REQUEST_FILENAME} !-f
    RewriteRule ^ index.php [L]

    # Handle Authorization Header
    RewriteCond %{HTTP:Authorization} .
    RewriteRule .* - [E=HTTP_AUTHORIZATION:%{HTTP:Authorization}]

    # Redirect old URLS to new ones...


    Redirect 301 /menus.blade.php https://cocktailsandcanapes.ca/menus

    Redirect 301 /corporate.blade.php https://cocktailsandcanapes.ca/catering/corporate



    # Redirect Permanent /event-catering-menus/ https://cocktailsandcanapes.ca/menus
    # Redirect Permanent /corporate-event-catering/ https://cocktailsandcanapes.ca/catering/corporate.html
    #Redirect Permanent /vancouver-wedding-caterers/ https://cocktailsandcanapes.ca/catering/weddings
    #Redirect Permanent /services /catering

</IfModule>
